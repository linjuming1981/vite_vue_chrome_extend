{
  "manifest_version": 3,
  "name": "linjuming chrome实验室",
  "version": "1.0.1",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "./src/background/background.js"
  },
  "permissions": [
    "webRequest",
    "tabs",
    "storage",
    "declarativeNetRequestFeedback",
    "declarativeNetRequest",
    "scripting"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}